;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns.deeznudd.local. admin.deeznudd.local. (
			      4		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	ns.deeznudd.local.
@	IN	A	192.168.10.4
ns	IN	A	192.168.10.2
www	IN	A	192.168.10.4
